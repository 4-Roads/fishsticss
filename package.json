{
  "name": "fishsticss",
  "version": "0.2.0",
  "author": "Trevor Blades",
  "description": "Fishsticss takes your old school CSS files and converts them to beautiful, functional LESS files.",
  "private": true,
  "scripts": {
    "start": "gulp dev",
    "dist": "gulp dist",
    "lint": "eslint src gulpfile.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "ignore": "node_modules",
          "plugins": [
            "transform-runtime"
          ],
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.0.16",
    "babel-preset-es2015": "^6.0.15",
    "babel-register": "^6.2.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.9.11",
    "browserify": "^12.0.1",
    "color": "^0.10.1",
    "eslint": "^1.8.0",
    "eslint-config-planet": "^4.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-uglify": "^1.4.2",
    "lodash": "^3.10.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
